;
; BIND data file for local loopback interface
;
$TTL	604800
@	IN	SOA	ns.lab.home. root.lab.home (
			      2		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	ns.lab.home.
NS	IN	A	192.168.2.2	
;
;DEBIAN SERVER
DEBIANSRV01	IN	A	192.168.2.2
;
;DEBIAN VM
DEBIANVM00	IN	A	192.168.2.30
DEBIANVM01	IN	A	192.168.2.31
DEBIANVM02	IN	A	192.168.2.32
DEBIANVM03	IN	A	192.168.2.33
DEBIANVM04	IN	A	192.168.2.34
DEBIANVM05	IN	A	192.168.2.35

;
;CENT OS VM
CENTOSVM01	IN	A	192.168.2.20
CENTOSVM02	IN	A	192.168.2.21
;
DEBIANTMP	IN	A	192.168.2.10
CENTOSTMP	IN	A	192.168.2.11
;
;SITES
WWW		IN	CNAME	VIP
SITE1		IN	CNAME	VIP
SITE2		IN	CNAME	VIP
