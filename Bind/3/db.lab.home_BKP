;
; BIND data file for local loopback interface
;
$TTL	604800
@	IN	SOA	DEBIANSRV01 root.lab.home. (
			      3		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 300 )	; Negative Cache TTL
;
@	IN	NS	DEBIANSRV01.
;
;DEBIAN SERVER
DEBIANSRV01	IN	A	192.168.2.2
;
;DEBIAN VM
DEBIANVM20	IN	A	192.168.2.20
