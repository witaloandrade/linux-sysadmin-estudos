@	IN	SOA	ns.lb.home. root.lab.home (
			      2		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	ns.lab.home.
ns	IN	A	192.168.2.2	


